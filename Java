    private void btuGuessAgainActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btuGuessAgainActionPerformed
        // TODO add your handling code here:
        
        count = 0;
        if (c1 == c2) {
            for (int y = 1; y <= 2; y++) {
                if (change[0] == 0) {
                    card1.setIcon(done);
                    change[0] = 2;
                    
                } else if (change[1] == 0) {
                    card2.setIcon(done);
                    change[1] = 2;
                    txtInstruction.setText("Congratulations");
                    
                } else if (change[2] == 0) {
                    card3.setIcon(done);
                    change[2] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[3] == 0) {
                    card4.setIcon(done);
                    change[3] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[4] == 0) {
                    card5.setIcon(done);
                    change[4] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[5] == 0) {
                    card6.setIcon(done);
                    change[5] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[6] == 0) {
                    card7.setIcon(done);
                    change[6] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[7] == 0) {
                    card8.setIcon(done);
                    change[7] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[8] == 0) {
                    card9.setIcon(done);
                    change[8] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[9] == 0) {
                    card10.setIcon(done);
                    change[9] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[10] == 0) {
                    card11.setIcon(done);
                    change[10] = 2;
                    txtInstruction.setText("Congratulations");
                } else if (change[11] == 0) {
                    card12.setIcon(done);
                    change[11] = 2;
                    txtInstruction.setText("Congratulations");
                } 
            }
        }
        else {

            for (int z = 1; z <= 2; z++) {

                if (change[0] == 0) {
                    card1.setIcon(back);
                    change[0] = 1;
                } else if (change[1] == 0) {
                    card2.setIcon(back);
                    change[1] = 1;
                } else if (change[2] == 0) {
                    card3.setIcon(back);
                    change[2] = 1;
                } else if (change[3] == 0) {
                    card4.setIcon(back);
                    change[3] = 1;
                } else if (change[4] == 0) {
                    card5.setIcon(back);
                    change[4] = 1;
                } else if (change[5] == 0) {
                    card6.setIcon(back);
                    change[5] = 1;
                } else if (change[6] == 0) {
                    card7.setIcon(back);
                    change[6] = 1;
                } else if (change[7] == 0) {
                    card8.setIcon(back);
                    change[7] = 1;
                } else if (change[8] == 0) {
                    card9.setIcon(back);
                    change[8] = 1;
                } else if (change[9] == 0) {
                    card10.setIcon(back);
                    change[9] = 1;
                } else if (change[10] == 0) {
                    card11.setIcon(back);
                    change[10] = 1;
                } else if (change[11] == 0) {
                    card12.setIcon(back);
                    change[11] = 1;
                }                    
            }
        }                        
    }//GEN-LAST:event_btuGuessAgainActionPerformed

